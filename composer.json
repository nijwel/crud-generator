{
    "name": "nijwel/crud-generator",
    "description": "A package to generate CRUD controllers, models, and migrations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nijwel",
            "email": "nijwel09@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Nijwel\\Crud\\": "src/"
        }
    },
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nijwel\\CrudGenerator\\providers\\CrudGeneratorServiceProvider"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php artisan nijwel:publish --tag=nijwel-crud --force"
        ]
    }
}
